{
  "expo": {
    "name": "Color Sleuth",
    "slug": "ColorSleuthApp",
    "description": "A game about choosing the color thats different!",
    "version": "2.3.0",
    "orientation": "portrait",
    "icon": "./assets/appIcon.png",
    "primaryColor": "#0f64fa",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "userInterfaceStyle": "automatic",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.matthewgardner.ColorSleuthApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.matthewgardner.ColorSleuthApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
